<!doctype html>
<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% tracks.map((track) =>{ %>
      <li>
        <%= track.title %>,
        <%= track.artist %>,
        <%= track.key %>,
        <%= track.bpm %>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
